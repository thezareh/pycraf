      "    def set_label(self, s):\n",
      "        \"\"\"\n",
      "        Set a label that will be displayed in the legend.\n",
      "\n",
      "        Parameters\n",
      "        ----------\n",
      "        s : object\n",
      "            *s* will be converted to a string by calling `str`.\n",
      "        \"\"\"\n",
      "        label = str(s) if s is not None else None\n",
      "        if label != self._label:\n",
      "            self._label = label\n",
      "            self.pchanged()\n",
      "            self.stale = True\n",
      "\n"
     ]
